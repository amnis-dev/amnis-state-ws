  "devDependencies": {
    "@amnis/eslint-config-node": "^1.3.3",
    "@amnis/tsconfig-node": "^1.3.3",
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "@reduxjs/toolkit": "^1.8.1",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "axios": "^0.27.2",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.15.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "hygen": "^6.2.0",
    "jest": "^28.1.0",
    "json-schema": "^0.4.0",
    "msw": "^0.40.1",
    "nanoid": "^3.3.4",
    "react-redux": "^8.0.1",
    "ts-jest": "^28.0.2",
    "ts-json-schema-generator": "^1.0.0",
    "ts-node": "^10.7.0",
    "type-fest": "^2.13.0",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "ts-node -P scripts/tsconfig.json scripts/build",
    "clean": "yarn clean:dist && yarn clean:ts",
    "clean:dist": "npx rimraf ./**/.dist",
    "clean:ts": "npx rimraf ./**/*.tsbuildinfo",
    "lint": "eslint --ext .ts --ext .tsx \"./\"",
    "test": "jest",
    "test:db": "jest packages/amnis-db/**/*",
    "test:init": "yarn test config:init",
    "test:watch": "yarn test --watch",
    "gen:entity": "hygen state entity --path packages/amnis-state/src",
    "set:version": "ts-node -P scripts/tsconfig.json scripts/version",
    "delete-node-modules": "npx rimraf ./**/node_modules"
  }
}
