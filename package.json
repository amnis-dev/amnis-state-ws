{
  "name": "@amnis/state-workspace",
  "version": "0.4.3",
  "repository": "https://github.com/amnis-dev/amnis-state",
  "author": "Eric Crowell <admin@amnis.dev>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "exports": {
    "import": "./dist/redux-toolkit.esm.mjs",
    "require": "./dist/index.js"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "tsc": "tsc",
    "release": "standard-version",
    "release:pre": "standard-version --prerelease alpha",
    "changelog": "lerna changelog",
    "build": "lerna run build",
    "pub": "npm publish --workspaces --access public",
    "clean": "yarn clean:dist && yarn clean:ts && yarn clean:rollup",
    "clean:dist": "npx rimraf ./**/dist ./**/lib ./**/srv",
    "clean:ts": "npx rimraf ./**/*.tsbuildinfo",
    "clean:rollup": "npx rimraf ./**/.rollup.cache",
    "lint": "tsc --noEmit && eslint --ext .ts --ext .tsx \"./\"",
    "test": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/",
    "test:core": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-core/",
    "test:db": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-db/",
    "test:fs": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-fs/",
    "test:auth": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-auth/",
    "test:api": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-api/",
    "test:state": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-state/",
    "test:root": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) test/*",
    "test:root:normie": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) test/normie.test.ts",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) --coverage",
    "start:client": "nodemon test/client",
    "start:server": "nodemon packages/amnis-express/test/server",
    "gen:core": "hygen state core --path packages/amnis-core/src",
    "gen:entity": "hygen state entity --path packages/amnis-state/src",
    "gen:schema": "ts-node -P scripts/tsconfig.json scripts/schema",
    "gen:validators": "ts-node -P scripts/tsconfig.json scripts/validators",
    "gen:code": "yarn gen:schema && yarn gen:validators",
    "set:version": "ts-node -P scripts/tsconfig.json scripts/version",
    "delete-node-modules": "npx rimraf ./**/node_modules"
  },
  "devDependencies": {
    "@amnis/eslint-config-node": "^2.0.0",
    "@amnis/tsconfig-node": "^2.0.0",
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "@reduxjs/toolkit": "^1.8.1",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^29.1.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.35",
    "ajv": "^8.11.0",
    "axios": "^0.27.2",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "bcrypt": "^5.0.1",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "eslint": "^8.15.0",
    "express": "^4.18.1",
    "file-type": "^18.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "hygen": "^6.2.0",
    "jest": "^29.2.0",
    "json-schema": "^0.4.0",
    "jsonwebtoken": "^8.5.1",
    "lerna": "^6.0.0",
    "morgan": "^1.10.0",
    "msw": "^0.47.4",
    "nanoid": "^3.3.4",
    "nodemon": "^2.0.16",
    "react-redux": "^8.0.1",
    "rollup": "^3.1.0",
    "standard-version": "^9.5.0",
    "ts-json-schema-generator": "^1.1.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "twitter-api-sdk": "^1.0.9",
    "type-fest": "^2.13.0",
    "typescript": "^4.8.4"
  }
}
