{
  "name": "@amnis/state-workspace",
  "version": "0.2.0-alpha.1",
  "main": "index.ts",
  "repository": "https://github.com/amnis-dev/amnis-state",
  "author": "Eric Crowell <eric.crowell@ecrow.dev>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@amnis/eslint-config-node": "^1.3.5",
    "@amnis/tsconfig-node": "^1.3.5",
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "@reduxjs/toolkit": "^1.8.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.35",
    "ajv": "^8.11.0",
    "axios": "^0.27.2",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "bcrypt": "^5.0.1",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "eslint": "^8.15.0",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "hygen": "^6.2.0",
    "jest": "^28.1.0",
    "json-schema": "^0.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "msw": "^0.40.1",
    "nanoid": "^3.3.4",
    "nodemon": "^2.0.16",
    "react-redux": "^8.0.1",
    "ts-jest": "^28.0.2",
    "ts-json-schema-generator": "^1.0.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "twitter-api-sdk": "^1.0.9",
    "type-fest": "^2.13.0",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "tsc": "tsc",
    "build": "ts-node -P scripts/tsconfig.json scripts/build",
    "pub": "ts-node -P scripts/tsconfig.json scripts/publish",
    "clean": "yarn clean:dist && yarn clean:ts",
    "clean:dist": "npx rimraf ./**/.dist",
    "clean:ts": "npx rimraf ./**/*.tsbuildinfo",
    "lint": "eslint --ext .ts --ext .tsx \"./\"",
    "test": "jest",
    "test:core": "jest packages/amnis-core/**/*",
    "test:db": "jest packages/amnis-db/**/*",
    "test:auth": "jest packages/amnis-auth/**/*",
    "test:api": "jest packages/amnis-api/**/*",
    "test:state": "jest packages/amnis-state/**/*",
    "test:root": "jest test/*",
    "test:root:normie": "jest test/normie.test.ts",
    "test:watch": "yarn test --watch",
    "start:client": "nodemon test/client",
    "gen:core": "hygen state core --path packages/amnis-core/src",
    "gen:entity": "hygen state entity --path packages/amnis-state/src",
    "gen:schema": "ts-node -P scripts/tsconfig.json scripts/schema",
    "gen:validators": "ts-node -P scripts/tsconfig.json scripts/validators",
    "gen:code": "yarn gen:schema && yarn gen:validators",
    "set:version": "ts-node -P scripts/tsconfig.json scripts/version",
    "delete-node-modules": "npx rimraf ./**/node_modules"
  }
}
