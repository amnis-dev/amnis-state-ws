{
  "name": "@amnis/state-workspace",
  "version": "0.8.3",
  "repository": "https://github.com/amnis-dev/amnis-state",
  "author": "Eric Crowell <admin@amnis.dev>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "exports": {
    "import": "./dist/redux-toolkit.esm.mjs",
    "require": "./dist/index.js"
  },
  "packageManager": "yarn@3.2.4",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "clean": "lerna run clean",
    "release": "standard-version -a",
    "release:pre": "standard-version -a --prerelease alpha",
    "release:patch": "standard-version -a --release-as patch",
    "release:minor": "standard-version -a --release-as minor",
    "release:major": "standard-version -a --release-as major",
    "pub": "npm publish --workspaces --access public",
    "lint": "tsc --noEmit && eslint --ext .ts --ext .tsx \"./\"",
    "t": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest)",
    "test": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/",
    "test:core": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-core/",
    "test:db": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-db/",
    "test:fs": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-fs/",
    "test:process": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-process/",
    "test:api": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-api/",
    "test:crypto": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-crypto/",
    "test:mock": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-mock/",
    "test:state": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-state/",
    "test:root": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) test/*",
    "test:root:normie": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) test/normie.test.ts",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) --coverage",
    "start:client": "nodemon test/client",
    "start:server": "nodemon packages/amnis-express/test/server",
    "gen:core": "hygen state core --path packages/amnis-core/src",
    "gen:entity": "hygen state entity --path packages/amnis-state/src",
    "gen:schema": "node scripts/schema.js",
    "gen:validators": "ts-node -P scripts/tsconfig.json scripts/validators",
    "gen:code": "yarn gen:schema && yarn gen:validators",
    "delete-node-modules": "npx rimraf ./**/node_modules"
  },
  "devDependencies": {
    "@amnis/eslint-config-node": "^2.1.0",
    "@amnis/tsconfig-node": "^2.1.0",
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "@reduxjs/toolkit": "^1.8.1",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^29.1.2",
    "@types/morgan": "^1.9.3",
    "ajv": "^8.11.0",
    "axios": "^0.27.2",
    "babel-jest": "^28.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "eslint": "^8.15.0",
    "express": "^4.18.1",
    "file-type": "^18.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "hygen": "^6.2.0",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.3.1",
    "json-schema": "^0.4.0",
    "lerna": "^6.0.0",
    "morgan": "^1.10.0",
    "msw": "^0.49.0",
    "nanoid": "^3.3.4",
    "nodemon": "^2.0.16",
    "rimraf": "^3.0.2",
    "rollup": "^3.1.0",
    "standard-version": "^9.5.0",
    "ts-json-schema-generator": "^1.1.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "twitter-api-sdk": "^1.0.9",
    "type-fest": "^2.13.0",
    "typescript": "^4.8.4"
  }
}
