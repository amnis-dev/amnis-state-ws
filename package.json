{
  "name": "@amnis/state-workspace",
  "version": "0.12.3",
  "repository": "https://github.com/amnis-dev/amnis-state",
  "author": "Eric Crowell <admin@amnis.dev>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "exports": {
    "import": "./dist/redux-toolkit.esm.mjs",
    "require": "./dist/index.js"
  },
  "packageManager": "yarn@3.4.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "clean": "lerna run clean",
    "release": "standard-version -a",
    "release:pre": "standard-version -a --prerelease alpha",
    "release:patch": "standard-version -a --release-as patch",
    "release:minor": "standard-version -a --release-as minor",
    "release:major": "standard-version -a --release-as major",
    "pub": "npm publish --workspaces --access public",
    "lint": "tsc --noEmit && eslint --ext .ts --ext .tsx \"./\"",
    "t": "vitest --run",
    "test": "vitest --run",
    "test:core": "vitest --run packages/amnis-core/",
    "test:process": "vitest --run packages/amnis-process/",
    "test:api": "vitest --run packages/amnis-api/",
    "test:crypto": "vitest --run packages/amnis-core/src/io/crypto/",
    "test:database": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-core/src/io/database",
    "test:filesystem": "yarn node --no-warnings --experimental-vm-modules $(yarn bin jest) packages/amnis-core/src/io/filesystem",
    "test:mock": "vitest --run packages/amnis-mock/",
    "test:state": "vitest --run packages/amnis-state/",
    "test:watch": "vitest --watch",
    "start:client": "nodemon test/client",
    "start:server": "nodemon packages/amnis-express/test/server",
    "gen:core": "hygen state core --path packages/amnis-core/src",
    "gen:entity": "hygen state entity --path packages/amnis-state/src",
    "gen:schema": "node scripts/schema.js",
    "gen:validators": "ts-node -P scripts/tsconfig.json scripts/validators",
    "gen:code": "yarn gen:schema && yarn gen:validators",
    "delete-node-modules": "npx rimraf ./**/node_modules"
  },
  "devDependencies": {
    "@amnis/eslint-config-node": "latest",
    "@amnis/tsconfig-node": "^2.1.0",
    "@reduxjs/toolkit": "alpha",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@yarnpkg/pnpify": "^4.0.0-rc.39",
    "ajv": "^8.11.0",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "hygen": "^6.2.0",
    "json-schema": "^0.4.0",
    "lerna": "^6.0.0",
    "msw": "^1.0.1",
    "nodemon": "^2.0.16",
    "redux": "^4.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.1.0",
    "standard-version": "^9.5.0",
    "ts-json-schema-generator": "^1.2.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "type-fest": "^2.13.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.4"
  }
}
