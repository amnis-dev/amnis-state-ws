{
  "presets": [
    [
      "@babel/preset-env", {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@amnis/core": "./packages/amnis-core/src",
          "@amnis/process": "./packages/amnis-process/src",
          "@amnis/api": "./packages/amnis-api/src",
          "@amnis/state": "./packages/amnis-state/src",
          "@reduxjs/toolkit/dist/redux-toolkit.esm.js": ["./node_modules/@reduxjs/toolkit/dist/redux-toolkit.umd.js"],
          "@reduxjs/toolkit/dist/query/rtk-query.esm.js": ["./node_modules/@reduxjs/toolkit/dist/query/rtk-query.umd.js"],
          "@reduxjs/toolkit/dist/query/react/rtk-query-react.esm.js": ["./node_modules/@reduxjs/toolkit/dist/query/react/rtk-query-react.umd.js"]
        }
      }
    ]
  ]
}