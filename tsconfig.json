{
  "extends": "@amnis/tsconfig-node",
  "compilerOptions": {
    "baseUrl": "./",
    "lib": ["DOM", "ES2021"],
    "esModuleInterop": true,
    "paths": {
      "@amnis/core": ["./packages/amnis-core/src/index.ts"],
      "@amnis/core/rtk.js": ["./packages/amnis-core/src/rtk.ts"],
      "@amnis/core/rtkq.js": ["./packages/amnis-core/src/rtkq.ts"],
      "@amnis/core/rtkqr.js": ["./packages/amnis-core/src/rtkqr.ts"],
      "@amnis/core/test/*": ["./packages/amnis-core/src/test/*"],
      "@amnis/db": ["./packages/amnis-db/src/index.ts"],
      "@amnis/process": ["./packages/amnis-process/src/index.ts"],
      "@amnis/api": ["./packages/amnis-api/src/index.ts"],
      "@amnis/state": ["./packages/amnis-state/src/index.ts"],
      "@reduxjs/toolkit": ["./node_modules/@reduxjs/toolkit/dist/index.d.ts"],
      "@reduxjs/toolkit/*": ["./node_modules/@reduxjs/toolkit/dist/*"],
      "@reduxjs/toolkit/query": ["./node_modules/@reduxjs/toolkit/dist/query/index.d.ts"],
      "@reduxjs/toolkit/query/*": ["./node_modules/@reduxjs/toolkit/dist/query/*"],
      "@reduxjs/toolkit/query/react": ["./node_modules/@reduxjs/toolkit/dist/query/react/index.d.ts"],
      "@reduxjs/toolkit/query/react/*": ["./node_modules/@reduxjs/toolkit/dist/query/react/*"],
    }
  },
  "include": ["./packages/**/*.ts*"]
}
