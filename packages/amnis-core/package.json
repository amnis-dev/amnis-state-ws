{
  "name": "@amnis/core",
  "version": "0.4.10",
  "license": "MIT",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/amnis-dev/amnis-state"
  },
  "homepage": "https://github.com/amnis-dev/amnis-state",
  "files": [
    "lib",
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./index.js": {
      "types": "./lib/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./rtk.js": {
      "types": "./lib/rtk.d.ts",
      "import": "./dist/rtk.mjs",
      "require": "./dist/rtk.js"
    },
    "./rtkq.js": {
      "types": "./lib/rtkq.d.ts",
      "import": "./dist/rtkq.mjs",
      "require": "./dist/rtkq.js"
    },
    "./rtkqr.js": {
      "types": "./lib/rtkqr.d.ts",
      "import": "./dist/rtkqr.mjs",
      "require": "./dist/rtkqr.js"
    }
  },
  "scripts": {
    "build": "yarn build:lib && yarn build:dist",
    "build:dist": "rollup --config rollup.config.mjs",
    "build:lib": "tsc -p tsconfig.build.json",
    "clean": "rimraf ./.rollup.cache ./dist ./lib ./types ./tsconfig.build.tsbuildinfo ./yarn-error.log"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.8.1"
  },
  "sideEffects": false
}