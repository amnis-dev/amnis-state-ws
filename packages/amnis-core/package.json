{
  "name": "@amnis/core",
  "version": "0.4.3",
  "license": "MIT",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/amnis-dev/amnis-state"
  },
  "homepage": "https://github.com/amnis-dev/amnis-state",
  "files": ["dist"],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./lib/*"
    }
  },
  "typesVersions": {
    ">=3.2": {
      "*": [
        "./lib/*"
      ]
    }
  },
  "scripts": {
    "build": "yarn build:lib && yarn build:dist",
    "build:dist": "rollup --config rollup.config.mjs",
    "build:lib": "tsc -p tsconfig.build.json",
    "clean": "rimraf ./.rollup.cache ./dist ./lib ./tsconfig.build.tsbuildinfo ./yarn-error.log"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.8.1"
  }
}