{
  "name": "@amnis/state",
  "version": "0.4.10",
  "license": "MIT",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/amnis-dev/amnis-state"
  },
  "homepage": "https://github.com/amnis-dev/amnis-state",
  "files": [
    "lib",
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./index.js": {
      "types": "./lib/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "yarn build:lib && yarn build:dist",
    "build:dist": "rollup --config rollup.config.mjs",
    "build:lib": "tsc -p tsconfig.build.json",
    "clean": "rimraf ./.rollup.cache ./dist ./lib ./types ./tsconfig.build.tsbuildinfo ./yarn-error.log"
  },
  "dependencies": {
    "@amnis/api": "0.4.10",
    "@amnis/core": "0.4.10"
  },
  "peerDependencies": {
    "@amnis/db": "0.4.10",
    "@amnis/fs": "0.4.10",
    "@reduxjs/toolkit": "^1.8.0",
    "react": "^18.0.0",
    "react-redux": "^8.0.2"
  },
  "peerDependenciesMeta": {
    "@amnis/db": {
      "optional": true
    },
    "@amnis/fs": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-redux": {
      "optional": true
    }
  },
  "sideEffects": false
}