{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/StateComplete",
  "definitions": {
    "StateComplete": {
      "type": "object",
      "properties": {
        "user": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/User"
          }
        },
        "session": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Session"
          }
        }
      },
      "additionalProperties": false
    },
    "User": {
      "type": "object",
      "properties": {
        "$id": {
          "$ref": "#/definitions/Reference",
          "description": "Identifier for this entity.",
          "default": ""
        },
        "created": {
          "$ref": "#/definitions/DateJSON",
          "description": "Creation date string.",
          "default": ""
        },
        "updated": {
          "$ref": "#/definitions/DateJSON",
          "description": "Updated date string.",
          "default": ""
        },
        "committed": {
          "type": "boolean",
          "description": "Flag to determine if the entity has been committed to storage.",
          "default": false
        },
        "$owner": {
          "$ref": "#/definitions/Reference",
          "description": "Entity that owns this data."
        },
        "$creator": {
          "$ref": "#/definitions/Reference",
          "description": "Possible user id creator of the entity."
        },
        "$updaters": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Reference"
          },
          "description": "Entities that have updated this."
        },
        "name": {
          "type": "string",
          "description": "Name for the user."
        },
        "email": {
          "type": "string",
          "description": "Email address",
          "default": ""
        },
        "$roles": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Reference%3CRole%3E"
          },
          "description": "Roles this user has been given."
        },
        "$permits": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Reference%3CPermit%3E"
          },
          "description": "Special-case permits this user has been bestowed."
        }
      },
      "required": [
        "$creator",
        "$id",
        "$owner",
        "$permits",
        "$roles",
        "$updaters",
        "committed",
        "created",
        "email",
        "name",
        "updated"
      ],
      "additionalProperties": false,
      "description": "Data associated to a User."
    },
    "Reference": {
      "type": "string",
      "description": "A id reference to another document."
    },
    "DateJSON": {
      "type": "string",
      "description": "A string that represents a JSON Date."
    },
    "Reference<Role>": {
      "type": "string",
      "description": "A id reference to another document."
    },
    "Reference<Permit>": {
      "type": "string",
      "description": "A id reference to another document."
    },
    "Session": {
      "type": "object",
      "properties": {
        "$id": {
          "$ref": "#/definitions/Reference",
          "description": "Identifier for this entity.",
          "default": ""
        },
        "created": {
          "$ref": "#/definitions/DateJSON",
          "description": "Creation date string.",
          "default": ""
        },
        "updated": {
          "$ref": "#/definitions/DateJSON",
          "description": "Updated date string.",
          "default": ""
        },
        "committed": {
          "type": "boolean",
          "description": "Flag to determine if the entity has been committed to storage.",
          "default": false
        },
        "$owner": {
          "$ref": "#/definitions/Reference",
          "description": "Entity that owns this data."
        },
        "$creator": {
          "$ref": "#/definitions/Reference",
          "description": "Possible user id creator of the entity."
        },
        "$updaters": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Reference"
          },
          "description": "Entities that have updated this."
        },
        "name": {
          "type": "string",
          "description": "Name of the session user."
        },
        "$user": {
          "$ref": "#/definitions/Reference%3CUser%3E",
          "description": "Reference to the User ID"
        },
        "expires": {
          "$ref": "#/definitions/DateJSON",
          "description": "Expiration date-time of the session."
        },
        "grants": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/GrantString"
          },
          "description": "Grants this session posesses."
        }
      },
      "required": [
        "$creator",
        "$id",
        "$owner",
        "$updaters",
        "$user",
        "committed",
        "created",
        "expires",
        "grants",
        "name",
        "updated"
      ],
      "additionalProperties": false,
      "description": "An interface for sessions."
    },
    "Reference<User>": {
      "type": "string",
      "description": "A id reference to another document."
    },
    "GrantString": {
      "type": "string",
      "description": "Role grant string."
    }
  }
}
