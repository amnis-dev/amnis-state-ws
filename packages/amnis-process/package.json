{
  "name": "@amnis/process",
  "version": "0.4.6",
  "license": "MIT",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/amnis-dev/amnis-state"
  },
  "homepage": "https://github.com/amnis-dev/amnis-state",
  "files": [
    "lib",
    "dist",
    "types"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./index.js": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "yarn build:lib && yarn build:dist",
    "build:dist": "rollup --config rollup.config.mjs",
    "build:lib": "tsc -p tsconfig.build.json",
    "clean": "rimraf ./.rollup.cache ./dist ./lib ./types ./tsconfig.build.tsbuildinfo ./yarn-error.log"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.8.1"
  },
  "dependencies": {
    "@amnis/core": "0.4.6",
    "ajv": "^8.11.0",
    "bcrypt": "^5.0.1",
    "cross-fetch": "^3.1.0",
    "jsonwebtoken": "^8.5.1"
  },
  "sideEffects": false
}