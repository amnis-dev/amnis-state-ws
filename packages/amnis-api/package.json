{
  "name": "@amnis/api",
  "version": "0.4.3",
  "license": "MIT",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/amnis-dev/amnis-state"
  },
  "homepage": "https://github.com/amnis-dev/amnis-state",
  "files": ["dist"],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "typesVersions": {
    ">=3.2": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "scripts": {
    "build": "yarn build:lib && yarn build:dist",
    "build:dist": "rollup --config rollup.config.mjs",
    "build:lib": "tsc -p tsconfig.build.json",
    "clean": "rimraf ./.rollup.cache ./dist ./lib ./tsconfig.build.tsbuildinfo ./yarn-error.log"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": "^1.8.1"
  },
  "dependencies": {
    "@amnis/core": "0.4.3",
    "@amnis/auth": "0.4.3",
    "@amnis/db": "0.4.3",
    "ajv": "^8.11.0",
    "cross-fetch": "^3.1.5"
  },
  "devDependencies": {
    "msw": "^0.40.1"
  }
}